<?xml version="1.0"?>
<robot name="quadcopter" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:property name="source" value="$(find last_letter_2)/config/param/aircrafts/quadcopter/rigidBody.yaml" />
<xacro:property name="dictionary" value="${load_yaml(source)}"/>
<xacro:property name="body_mass" value="${dictionary['airframe/m']}"/>

<!-- body, airfoil sizes -->
<xacro:property name="body_s" value="1" />   

<!-- body link parameters x, y, z, mass-->
<xacro:property name="body_x" value="${body_s}" />
<xacro:property name="body_y" value="${body_s}" />
<xacro:property name="body_z" value="${body_s/5}" />
<xacro:property name="body_m" value="${body_mass}" />

    <link name="body_FLU">
        <visual>
        <origin rpy="0 0 0.785"/>
            <geometry>
               <box size="${body_x} ${body_y} ${body_z}" />
            </geometry>
        </visual>

        <collision>
        <origin rpy="0 0 0.785"/>
            <geometry>
               <box size="${body_x} ${body_y} ${body_z}" />
            </geometry>
        </collision>
        
        <inertial>
        <origin rpy="0 0 0.785"/>
            <mass value="${body_m}" />
             <inertia   ixx="${1/12*body_m*(body_y*body_y+body_z*body_z)}" ixy="0" ixz="0" iyz="0"
                        iyy="${1/12*body_m*(body_x*body_x+body_z*body_z)}" 
                        izz="${1/12*body_m*(body_x*body_x+body_y*body_y)}" />
        </inertial> 

    </link>

---------------------------------------------------------- 4 quadcopter arms
<xacro:property name="arm_x" value="${2*body_s}" />
<xacro:property name="arm_y" value="${0.1*body_s}" />
<xacro:property name="arm_z" value="${0.1*body_s}" />
<xacro:property name="arm_m" value="${0.1*body_m}" />

    <link name="arm1">
        <visual>
        <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
               <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </visual>

        <collision>
        <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
               <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="${arm_x/2} 0 0"/>
            <mass value="${arm_m}" />
            <inertia    ixx="${1/12*arm_m*(arm_y*arm_y+arm_z*arm_z)}" ixy="0" ixz="0" iyz="0"
                        iyy="${1/12*arm_m*(arm_x*arm_x+arm_z*arm_z)}" 
                        izz="${1/12*arm_m*(arm_x*arm_x+arm_y*arm_y)}" />
        </inertial> 
    </link>

    <joint name="body_FLU_to_arm1" type="fixed">
        <origin xyz="${0.9*body_x/2} 0 0" rpy="0 0 0"/>
        <parent link="body_FLU" />
        <child link="arm1" />
    </joint>
    
    <link name="arm2">
        <visual>
        <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
                <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </visual>

        <collision>
            <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
                <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="${arm_x/2} 0 0"/>
            <mass value="${arm_m}" />
            <inertia    ixx="${1/12*arm_m*(arm_y*arm_y+arm_z*arm_z)}" ixy="0" ixz="0" iyz="0"
                        iyy="${1/12*arm_m*(arm_x*arm_x+arm_z*arm_z)}" 
                        izz="${1/12*arm_m*(arm_x*arm_x+arm_y*arm_y)}" />
        </inertial> 
    </link>

    <joint name="body_FLU_to_arm2" type="fixed">
        <origin xyz="0 -${0.9*body_y/2} 0" rpy="0 0 ${3/2*pi}"/>
        <parent link="body_FLU" />
        <child link="arm2" />
    </joint>

        <link name="arm3">
        <visual>
        <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
                <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </visual>

        <collision>
            <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
                <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="${arm_x/2} 0 0"/>
            <mass value="${arm_m}" />
            <inertia    ixx="${1/12*arm_m*(arm_y*arm_y+arm_z*arm_z)}" ixy="0" ixz="0" iyz="0"
                        iyy="${1/12*arm_m*(arm_x*arm_x+arm_z*arm_z)}" 
                        izz="${1/12*arm_m*(arm_x*arm_x+arm_y*arm_y)}" />
        </inertial> 
    </link>

    <joint name="body_FLU_to_arm3" type="fixed">
        <origin xyz="-${0.9*body_x/2} 0 0" rpy="0 0 ${pi}"/>
        <parent link="body_FLU" />
        <child link="arm3" />
    </joint>

        <link name="arm4">
        <visual>
        <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
                <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </visual>

        <collision>
            <origin xyz="${arm_x/2} 0 0"/>
            <geometry>
                <box size="${arm_x} ${arm_y} ${arm_z}" />
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="${arm_x/2} 0 0"/>
            <mass value="${arm_m}" />
            <inertia    ixx="${1/12*arm_m*(arm_y*arm_y+arm_z*arm_z)}" ixy="0" ixz="0" iyz="0"
                        iyy="${1/12*arm_m*(arm_x*arm_x+arm_z*arm_z)}" 
                        izz="${1/12*arm_m*(arm_x*arm_x+arm_y*arm_y)}" />
        </inertial> 
    </link>

    <joint name="body_FLU_to_arm4" type="fixed">
        <origin xyz="0 ${0.9*body_y/2} 0" rpy="0 0 ${1/2*pi}"/>
        <parent link="body_FLU" />
        <child link="arm4" />
    </joint>

    ------------------------------------ motors


<xacro:property name="motor_x" value="${0.2*body_s}" />
<xacro:property name="motor_y" value="${0.2*body_s}" />
<xacro:property name="motor_z" value="${0.2*body_s}" />
<xacro:property name="motor_m" value="${0.1*body_m}" />

<!-- only motor(Number_0f_Motor) acceptable name for motors -->
     <link name="motor1">
        <visual>
        <origin xyz="${motor_x/2} 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="${motor_x} ${motor_y} ${motor_z}" />
            </geometry>
        </visual>

        <collision>
            <origin xyz="${motor_x/2} 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="${motor_x} ${motor_y} ${motor_z}" />
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="${motor_x/2} 0 0" rpy="0 0 0"/>
            <mass value="${motor_m}" />
            <inertia    ixx="${1/12*motor_m*(motor_y*motor_y+motor_z*motor_z)}" ixy="0" ixz="0" iyz="0"
                        iyy="${1/12*motor_m*(motor_x*motor_x+motor_z*motor_z)}" 
                        izz="${1/12*motor_m*(motor_x*motor_x+motor_y*motor_y)}" />
        </inertial> 
    </link>

    <joint name="arm1_to_motor1" type="revolute">
        <origin xyz="${arm_x-motor_x/2} 0 ${arm_z/2}" rpy="0 -1.57 0"/>
        <parent link="arm1" />
        <child link="motor1" />
        <!-- <limit lower="-${pi/4}" upper="${pi/4}" effort="10" velocity="3"/> -->
        <limit effort="1000.0" lower="-0.000872665" upper="0.000872665" velocity="0.5"/>
    </joint>

